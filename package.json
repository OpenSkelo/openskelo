{
  "name": "openskelo",
  "version": "0.1.0",
  "description": "Give your AI agents a backbone. Deterministic pipeline runtime â€” local-first, config-driven, zero cost.",
  "type": "module",
  "bin": {
    "skelo": "./dist/cli.js",
    "openskelo": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "preinstall": "node scripts/check-node.mjs",
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/cli.js start",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:report": "npm run test:coverage && vitest run --reporter=json --outputFile=docs/reports/vitest-results.json && node scripts/generate-test-report.mjs",
    "check:genericity": "bash scripts/check-genericity.sh",
    "demo:viral": "bash scripts/run-viral-demo.sh"
  },
  "keywords": [
    "ai",
    "agents",
    "pipeline",
    "orchestration",
    "local-first",
    "deterministic",
    "quality-gates",
    "ci-cd",
    "llm",
    "ollama",
    "openai",
    "openclaw",
    "multi-agent",
    "workflow"
  ],
  "author": "OpenSkelo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenSkelo/openskelo.git"
  },
  "homepage": "https://github.com/OpenSkelo/openskelo",
  "bugs": {
    "url": "https://github.com/OpenSkelo/openskelo/issues"
  },
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@hono/node-server": "^1.0.0",
    "acorn": "^8.15.0",
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "hono": "^4.0.0",
    "nanoid": "^5.0.0",
    "ws": "^8.16.0",
    "yaml": "^2.4.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  }
}
